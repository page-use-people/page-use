{
    "name": "@page-use/core",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.mjs",
    "types": "dist/index.d.mts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.mts"
        }
    },
    "imports": {
        "#core/*": "./dist/*"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "build:no-check": "tsc --noCheck",
        "dev": "concurrently -n tsc,node -c blue,green \"tsc --watch --preserveWatchOutput\" \"nodemon --watch dist --ext mjs --delay 2 --exec 'node --inspect=0.0.0.0:14001' dist/index.mjs\"",
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
        "db:migrate:make": "kysely migrate:make",
        "db:migrate:latest": "kysely migrate:latest",
        "db:migrate:up": "kysely migrate:up",
        "db:migrate:down": "kysely migrate:down",
        "db:migrate:refresh": "[ \"$NODE_ENV\" != production ] && kysely migrate:rollback --all && kysely migrate:latest || echo 'Refused: NODE_ENV is production'",
        "db:seed:make": "kysely seed:make",
        "db:seed:run": "kysely seed:run",
        "db:generate": "kysely-codegen --dialect postgres --url $DATABASE_URL --out-file src/db/generated.ts"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "0.39.0",
        "@t3-oss/env-core": "0.13.10",
        "@trpc/server": "11.8.1",
        "bottleneck": "2.19.5",
        "cors": "2.8.5",
        "diff": "7.0.0",
        "express": "5.1.0",
        "ioredis": "5.9.2",
        "jose": "6.0.11",
        "kysely": "0.28.2",
        "liquidjs": "10.21.0",
        "mitt": "3.0.1",
        "pg": "8.18.0",
        "posthog-node": "5.24.15",
        "pure-rand": "7.0.1",
        "uuid25": "0.1.6",
        "uuidv7": "1.0.2",
        "winston": "3.19.0",
        "zod": "4.3.6"
    },
    "devDependencies": {
        "@types/cors": "2.8.19",
        "@types/diff": "6.0.0",
        "@types/express": "5.0.6",
        "@types/jest": "29.5.14",
        "@types/node": "24.0.0",
        "@types/pg": "8.16.0",
        "concurrently": "9.2.1",
        "jest": "29.7.0",
        "kysely-codegen": "0.18.2",
        "kysely-ctl": "0.20.0",
        "nodemon": "3.1.11",
        "prettier": "3.8.1",
        "ts-jest": "29.4.6",
        "tsx": "4.19.4",
        "typescript": "5.9.3"
    }
}
